                                <script>
                                    {% if errors != null %}
                                    {% for err in errors %}
                                    $.notify({
                                        position: 8,
                                        type: 'error',
                                        autoClose: false,
                                        message: "{{ err.msg }}"
                                    });
                                    {% endfor %}
                                    {% endif %}

                                    {% if error %}
                                    $.notify({
                                        position: 3,
                                        type: 'warn',
                                        message: "{{ error }}",
                                        duration: 10000
                                    });
                                    {% endif %}
                                </script>
